{
  "name": "@ccreusat/babel-lib",
  "version": "0.0.1",
  "description": "babel-lib conf",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "require": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prebuild": "rm -rf dist",
    "build": "npm run prebuild && npm run build:js && rollup -c",
    "#build": "concurrently npm:build:*",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:esm": "BABEL_ENV=esm babel src --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "BABEL_ENV=cjs babel src --extensions .ts,.tsx -d dist/cjs --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccreusat/ccre-ui.git"
  },
  "keywords": [
    "babel",
    "react",
    "components"
  ],
  "author": "Clement Creusat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ccreusat/ccre-ui/issues"
  },
  "homepage": "https://github.com/ccreusat/ccre-ui#readme",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/react": "^18.0.17",
    "react": "^18.2.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "concurrently": "^7.3.0"
  }
}
