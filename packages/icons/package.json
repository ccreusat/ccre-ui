{
  "name": "@ccreusat/icons",
  "version": "1.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run svgr && vite build",
    "preview": "vite preview",
    "svgr": "svgr --icon --replace-attr-values '#4A4A4A=currentColor' --jsx-runtime automatic --title-prop --typescript --out-dir src src/icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccreusat/ccre-ui.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/@ccreusat"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.3.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "clsx": "^1.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ccre-ui-icons.umd.cjs",
  "module": "./dist/ccre-ui-icons.js",
  "types": "./dist/ccre-ui-icons.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ccre-ui-icons.js",
      "require": "./dist/ccre-ui-icons.umd.cjs"
    }
  },
  "bugs": {
    "url": "https://github.com/ccreusat/ccre-ui/issues"
  },
  "homepage": "https://github.com/ccreusat/ccre-ui#readme"
}
