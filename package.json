{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "description": "@ccreusat react monorepo",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "turbo run dev"
  },
  "workspaces": [
    "packages/*",
    "docs/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ccreusat/ccre-ui.git"
  },
  "keywords": [
    "react",
    "frontend",
    "storybook",
    "components",
    "ui",
    "vite"
  ],
  "author": "Clement Creusat",
  "bugs": {
    "url": "https://github.com/ccreusat/ccre-ui/issues"
  },
  "homepage": "https://github.com/ccreusat/ccre-ui#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "multi-semantic-release": "^2.13.0",
    "semantic-release": "^19.0.3",
    "semantic-release-monorepo": "^7.0.5",
    "turbo": "^1.4.5",
    "typescript": "^4.7.4"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer"
    ],
    "branches": [
      "master",
      "feature",
      "next"
    ],
    "noCi": true
  }
}
